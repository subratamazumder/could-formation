sudo yum install http://pkg.jenkins-ci.org/redhat/jenkins-2.3-1.1.noarch.rpm -y
sudo sed -i -e 's/JENKINS_JAVA_OPTIONS="/JENKINS_JAVA_OPTIONS="-Dhudson.diyChunking=false /g' /etc/sysconfig/jenkins
sudo chkconfig jenkins on
sudo service jenkins start
sleep 60
cd /var/lib/jenkins
sudo sed -i -e 's/<denyAnonymousReadAccess>true</<denyAnonymousReadAccess>false</g' config.xml
sudo service jenkins restart
sleep 60
sudo curl -O http://localhost:8080/jnlpJars/jenkins-cli.jar
